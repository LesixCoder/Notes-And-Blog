{
  "extends": "eslint-config-egg",
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": ["error"],
    "space-before-function-paren": "off",
    "array-bracket-spacing": "off",
    "no-unused-vars": "off",
    "semi": "off",
    "comma-dangle": "off",
    "arrow-parens": "off"
  },
  "globals": {
    "use": true,
    "Controller": true
  }
}
